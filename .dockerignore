# Ignore git files
.git/
.gitignore

# Ignore docker files (optional, but good practice to keep context clean)
docker-compose.yml
Dockerfile
Dockerfile.celery
.dockerignore

# Ignore python cache and venv
__pycache__/
*.py[cod]
*$py.class
venv/
.venv/
env/
.env

# Ignore node modules and frontend build artifacts
node_modules/
frontend/node_modules/
frontend/dist/
frontend/.env
frontend/.env.*

# Ignore media and static files (handled by volumes or collected during build)
media/
staticfiles/
staticfiles_collected/

# Ignore specific OS files
.DS_Store
Thumbs.db

# Ignore IDE files
.vscode/
.idea/

# Ignore certificates (generated or mounted at runtime)
certs/

# Ignore documentation
docs/
Readme.md
